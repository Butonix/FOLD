  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  margin: 0;
  background-color: @background-color;
  transition: color 0.25s ease;
  transition: background-color 0.25s ease;

  line-height: 23px;
  font-size: 15px;
  &.read, &.edit, &.embed{
    min-height: 1420px;
    @media @desktop-xtall {
      min-height: 2000px;
    }
    overflow-x: hidden;

    @media @mobile-or-thin-window {
      background-color: @unselected-link-color;
    }

  }
}

.story-footer{
