      button{
        .size(25px);
      }
    }
    .random-story{
      margin-top: 20px;
      pointer-events: auto;
    }
  }
  p{
    display: inline-block;
    margin-top: -10px;
  }
}

.full-page-width{
  overflow: hidden;
  width: 100%;
}


div#overlay {
  background: transparent url("https://res.cloudinary.com/fold/image/upload/v1/static/overlay.png");
  opacity: 0.5;
