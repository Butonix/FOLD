@marker-height: 5px;

.vertical-marker{
  width: @marker-width;
  margin: auto;
  height: @marker-height;
  background-color: @medium-color;
  &.selected{
    background-color: @action-color;
  }
}

.mobile-minimap-horizontal{
  position: fixed;
  background-color: @background-color;
  bottom:0;
  right:0;
  z-index: 22;
  .box-shadow(0px -3px 8px 0px rgba(0,0,0,0.2));
  &:after {
    content:"";
    position: absolute;
    z-index: -1;
    left: 0;
    right: 0;
    bottom: @border-bottom;
    border-bottom: 1px solid @medium-color;
  }
}

.mobile-minimap-horizontal-table{
  .size-to-fit;
  display: table;
  bottom: 0px;
  right: 0px;
}


.horizontal-marker-row{
  height: 100%;
  display: table-row;
}

.horizontal-marker-cell{
  display: table-cell;
  vertical-align: middle;
  text-align: center;
}

.horizontal-marker{
  width: @marker-width;
  margin: auto;
  height: @marker-height;
  background-color: @medium-color;
  &.selected{
    background-color: @action-color;
  }
}


.mobile-minimap-corner{
  position: fixed;
  z-index: 24;
  background-color: @background-color;
  bottom: 0;
  left: 0;
  &:before { // complete vertical line
    content:"";
    position: absolute;
    z-index: -1;
    left: @border-left;
    right: 0;
    top: 0;
    bottom: 0;
    border-left: 1px solid @medium-color;
  }
  &:after { // complete horizontal line
    content:"";
    position: absolute;
    z-index: -1;
    left: @border-left;
    right: 0;
    top: 0;
    bottom: @border-bottom;
    border-bottom: 1px solid @medium-color;
  }
}

.publish-overlay {
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(255,255,255,0.4);
  z-index: 999999999;
  vertical-align: middle;
}

.central-publish-modal{
  background-color: @dark-color;
  padding: @nonMobileNarrativeSectionPadding;
  width: 600px;
  max-width: 100%;
  .center-over-parent-div;
  color: @white-color;

  label {
    font-size: 14px;
  }

  input[type=text] {
    font-size: 15px;
  }

  input[type=file] {
    &::before {
      display: inline-block;
      background: @social-color;
      color: @white-color;
    }
    &:hover::before {
      background-color: @action-color;
      color: @white-color;
    }
  }
  p{
    font-size: 12px;
  }
  input[type=text], input[type=file]{
    margin-bottom: 15px;
  }
  .cancel-publish{
    position: absolute;
    top: 4px;
    right: 4px;
    font-size: 20px;
  }
  .confirm-publish{
    float: right;
    //position: absolute;
    //bottom: @nonMobileNarrativeSectionPadding;
    //right: @nonMobileNarrativeSectionPadding;
    background: @social-color;
    color: @white-color;
    line-height: 35px;
    padding-top: 5px;
    padding-bottom: 5px;
    padding-left: 14px;
    padding-right: 14px;
    &:hover {
      background-color: @action-color;
      color: @white-color;
    }
  }
}


.mobile-progress-header{
  text-align: center;
  position: absolute;
  width: 100%;
  background-color: rgba(50,50,50,0.6);
  color: @medium-color;
  z-index: 1000;
}

.mobile-context-back-button{
  @size: 38px;
  float:right;
  .size(@size);
  padding: 0;
  position: relative;
  margin-right: 0px;
  margin-top: 10px;
}

div.tagsinput {
  .FFMarkWeb;
  font-size: 15px;
  border:1px solid #CCC;
  background: #FFF;
  padding:5px;
  overflow-y: auto;
}

div.tagsinput span.tag {
  display: block;
  float: left;
  padding: 5px;
  padding-left: 10px;
  padding-right: 10px;
  text-decoration:none;
  background: @action-color;
  color: @white-color;
  margin-right: 8px;
  margin-bottom:5px;
}

div.tagsinput span.tag a {
  opacity: .8;
}

div.tagsinput input {
  width:80px;
  margin:0px;
  border:1px solid transparent;
  padding:5px;
  background: transparent;
  color: #000;
  outline:0px;
  margin-right:5px;
  margin-bottom:5px;
}

div.tagsinput div {
  display:block;
  float: left;
}

.tags_clear {
  clear: both;
  width: 100%;
  height: 0px;
}

.not_valid {background: #FBD8DB !important;
  color: #90111A !important;}

.pull-right {
  float: right;
}


div.inline-block {
  display: inline-block;
}

div.collapse {
  display: none !important;
}


body.konami{

  @keyframes blink {
    0% { color: yellow; }
    100% { color: green; }
  }
  @-webkit-keyframes blink {
    0% { color: yellow; }
    100% { color: green; }
  }
  * {
    -webkit-animation: blink 0.5s linear infinite;
    -moz-animation: blink 0.5s linear infinite;
    animation: blink 0.5s linear infinite;
  }

  div:nth-of-type(1)  { animation: wave 17s   0.000s linear infinite; }
  div:nth-of-type(2)  { animation: wave 17s -16.227s linear infinite; }
  div:nth-of-type(3)  { animation: wave 17s -15.455s linear infinite; }
  div:nth-of-type(4)  { animation: wave 17s -14.682s linear infinite; }
  div:nth-of-type(5)  { animation: wave 17s -13.909s linear infinite; }
  div:nth-of-type(6)  { animation: wave 17s -13.136s linear infinite; }
  div:nth-of-type(7)  { animation: wave 17s -12.364s linear infinite; }
  div:nth-of-type(8)  { animation: wave 17s -11.591s linear infinite; }
  div:nth-of-type(9)  { animation: wave 17s -10.818s linear infinite; }
  div:nth-of-type(10) { animation: wave 17s -10.045s linear infinite; }
  div:nth-of-type(11) { animation: wave 17s  -9.273s linear infinite; }
  div:nth-of-type(12) { animation: wave 17s  -8.500s linear infinite; }
  div:nth-of-type(13) { animation: wave 17s  -7.727s linear infinite; }
  div:nth-of-type(14) { animation: wave 17s  -6.955s linear infinite; }
  div:nth-of-type(15) { animation: wave 17s  -6.182s linear infinite; }
  div:nth-of-type(16) { animation: wave 17s  -5.409s linear infinite; }
  div:nth-of-type(17) { animation: wave 17s  -4.636s linear infinite; }
  div:nth-of-type(18) { animation: wave 17s  -3.864s linear infinite; }
  div:nth-of-type(19) { animation: wave 17s  -3.091s linear infinite; }
  div:nth-of-type(20) { animation: wave 17s  -2.318s linear infinite; }
  div:nth-of-type(21) { animation: wave 17s  -1.545s linear infinite; }
  div:nth-of-type(22) { animation: wave 17s  -0.773s linear infinite; }

  @keyframes wave {
    0%   { left:-2%; background: #3B44D1; }
    5%   { background: #9337FE; }
    10%  { height:10%; margin-top: -5%; background: #C532FC; }
    15%  { background: #F639F8; }
    20%  { height:34%; margin-top:-17%; background: #F236C8; }
    25%  { background: #FF2F8D; }
    30%  { height:10%; margin-top: -5%; background: #EE3860; }
    35%  { background: #DC5245; }
    40%  { height:34%; margin-top:-17%; background: #F38643; }
    45%  { background: #F8B435; }
    50%  { height:10%; margin-top: -5%; background: #FAF444; }
    55%  { background: #E0FF3B; }
    60%  { height:34%; margin-top:-17%; background: #E1FF3C; }
    65%  { background: #46F443; }
    70%  { height:10%; margin-top: -5%; background: #54E67B; }
    75%  { background: #4DF3A9; }
    80%  { height:34%; margin-top:-17%; background: #3AF9DA; }
    85%  { background: #36EBF4; }
    90%  { height:10%; margin-top: -5%; background: #3DB3F3; }
    95%  { background: #3C82F1; }
    100% { height:34%; margin-top:-17%; left:100%; background: #5B38EE; }
  }
}

div.loading-container {
  margin-top: 100px;
  .display-flex;
  align-items: center;
  justify-content: center;
  &.header {
    margin-top: 6px;
    position: absolute;
    top: 20px;
    right: 195px;
  }
  i.loading-icon {
    z-index: 50;
  }
}

.loading-page{
  position: fixed;
  top: 0;
  left: 0;
  .centered-loading-icon{
    &::before{
      height: 47%;
      display: block;
      content: '';
    }
    .size-to-fit;
    text-align: center;
  }
}

.banner-buttons{

  div.action-items {
    @height: @magic-styling-distance;
    font-size: 13px;
    position: absolute;

    @media @mobile{
      width: 100%;
      text-align: center;
    }
    &.slim{
      top: 0px;
      right: 0;
    }
    &.fat{
      top: @magic-styling-distance;
      @media @not-mobile{
        right: 140px;
        @media @home-banner-break-1{
          right: 40px;
        }
      }
    }

    color: @white-color;
    .FFMarkWebBold;

    button{
      padding: 0px 23px;
      width: 180px;
      height: 100%;
      &:hover {
        background-color: @dark-color;
      }
    }

    a {
      width: 180px;
      height: 100%;
      &:hover {
        background-color: @dark-color;
      }
    }

    .create-story {
      background-color: @action-color;
      &:hover {
        background-color: @dark-color;
      }
    }

    li {
      height: @height;
      @media @not-mobile{
        float: left;
      }
      display: inline-block;
      position: relative;
    }

    &.fat{
      .action-item-list>li{
        &:not(:last-child) {
          margin-right: @magic-styling-distance / 2;
        }
        @media @home-banner-break-2{
          &:last-child {
            display: none;
          }
          &:nth-last-child(2) {
            margin-right: 0;
          }
        }
      }
    }

    .user-action {
      background-color: @social-color;
      @profile-picture-size: 26px;
      button{
        line-height: @height;
      }
      span{
        margin: auto;
        display: inline-block;
      }
      .text{
        max-width: 180px;
      }
      img.profile-picture {
        margin:auto;
        margin-right: 2px;
        margin-bottom: -10px;
        z-index: 10;
        border-radius: @profile-picture-size/2;
        width: @profile-picture-size;
        height: auto;
      }
    }

    .about-fold {
      background-color: @orange-color;
    }

    ol.user-menu {
      background-color: @social-color;
      width: 180px;
      li {
        float: none;
        display: block;
        margin:0;
        width: 180px;
        border-top: 1px solid @white-color;
      }
    }
  }
  .social-links{
    position: absolute;
    top: @magic-styling-distance;
    right: @magic-styling-distance;
    li{
      display: inline-block;
      width: @magic-styling-distance;
      text-align: center;
    }
    @media @home-banner-break-1 {
      display: none;
    }
  }
}

.read-admin-ui{
  position: fixed;
  bottom: 0;
  right: 200px;
  color: @black-color;
}

.admin{
  padding: 20px;
  input{
    margin-top: 100px;
    display: block;
    width: 300px;
  }
}

.admin-recent-drafts, .admin-recent-activities{
  .show-more{
    background-color: @black-color;
    position: fixed;
    right: 0;
    bottom: 0;
    z-index: 1;
  }
}

.random-story, .random-person{
  height: @magic-styling-distance;
  color: @white-color;
  .FFMarkWebBold;
  font-size: 13px;
  background-color: @social-color;
  width: 210px;
  display: inline-block;
  button{
    .size-to-fit;
  }
  svg{
    @keyframes spin{
      0%{
        .transform(rotate(0deg));
      }
      100%{
        .transform(rotate(360deg));
      }
    }
    .animation(spin 0.9s 1);
    height: 100%;
  }
}

.context-overlay {
  position: fixed;
  z-index: 300;
  background-color: fade(@white-color, 95%);
  @media @mobile {
    background-color: @black-color;
  }
  left: 0;
  top: 0;
  cursor: pointer;
  .text-content{
    .size-to-fit;
    background-color: @white-color;
    overflow-y: scroll;
    padding: 20px;
    padding-top: 40px;
    position: relative;
    &:before{
      content: 'x';
      position: absolute;
      background-color: @black-color;
      color: @white-color;
      .size(20px);
      top: 20px;
      right: 20px;
      font-size: 20px;
      font-weight: normal;
      line-height: 18px;
      text-align: center;
    }
  }
}

.context-overlay-content{
  .center-over-parent-div;
  width: 80%;
  height: 80%;
  @media @mobile {
    .size-to-fit;
  }

  img, video {
    width: auto;
    max-width: 100%;
    margin: auto;
    vertical-align: middle;
    display: inline-block;
    max-height: 100%;
    .center-over-parent-div;
    z-index: 2;
  }
  .loading-icon{
    .center-over-parent-div;
    z-index: 1;
  }
}


.follow-button{
  height: @magic-styling-distance;
  svg{
    float: left;
    height: 18px;
    margin-top: -3px;
    margin-right: 10px;
  }
  button{
    .FFMarkWebBold;
    font-size: 13px;
    .size-to-fit;
    &.unfollow{
      color: @social-color;
      .fg{
        fill: @social-color;
      }
      &:hover{
        color: @black-color;
        .fg{
          fill: @black-color;
        }
      }
    }
    &.follow{
      color: @medium-color;
      .fg{
        fill: @medium-color;
      }
      &:hover{
        color: @social-color;
        .fg{
          fill: @social-color;
        }
      }
    }

    &.unfollow:hover{
      &:not(.just-followed){
        span{
          display: none;
        }
        &:after{
          content: 'Unfollow';
        }
        color: @orange-color;
        .fg{
          fill: @orange-color;
        }
      }
      &.just-followed{
        color: @social-color;
        .fg{
          fill: @social-color;
        }
      }
    }
  }

  &.bar-style{
    button{
      white-space: nowrap;
      .FFMagdaCleanMonoWebProRegular;
      font-size: 13px;
      padding: 0;
      text-align: left;
    }
    &.unfollow-style{
      width: 95px;
    }
  }

  &:not(.bar-style){
    width: 180px;
    display: inline-block;
    button{
      .FFMarkWebBold;
      color: @white-color !important;
      font-size: 13px;
      .size-to-fit;
      &.unfollow{
        background-color: @social-color;
        &:hover{
          background-color: @black-color;
        }
      }
      &.follow{
        background-color: @medium-color;
        &:hover{
          background-color: @social-color;
        }
      }

      &.unfollow:hover{
        &:not(.just-followed){
          span{
            display: none;
          }
          &:after{
            content: 'Unfollow';
          }
          background-color: @orange-color;
        }
        &.just-followed{
          background-color: @social-color;
        }
      }
    }
  }
}

.not-found{
  position: absolute;
  .content{
    .center-over-parent-div;
    text-align: center;
    line-height: normal;
  }
}

.fit{
  .size-to-fit;
}

.amaran-wrapper{
  z-index: 9999999;
}

.unsubscribe{
  .rest{
    padding-top: 150px;
    text-align: center;
    button{
      width: 180px;
      height: @magic-styling-distance;
      background-color: @action-color;
      margin-left: 15px;
    }
    .padding-sides(20px);
  }
}

.mobile-embed-placeholder{
  background-color: @action-color;
  a{
    .size-to-fit;
    display: block;
    span{
      .center-over-parent-div;
    }
  }
}
