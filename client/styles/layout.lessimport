@minimap-right-buffer: 6px;
@minimap-bottom-buffer: @magic-styling-distance + 10px;
@minimap-top-buffer: 20px;
@minimap-padding: 6px;

div.minimap {
  .background-transition;
  position: fixed;
  overflow: hidden;
  z-index: 60;
  right: @minimap-right-buffer;
  bottom: @minimap-bottom-buffer;
  padding: @minimap-padding;

  .expand {
    .opacity-transition;
    position: absolute;
    top: 6px;
    right: 6px;
    opacity: 1.0;
    background-color: #323232;
    width: 30px;
    height: 30px;
    font-size: 21px;
    border-radius: 3px;
    padding-top: 5px;
    padding-left: 6px;
    opacity: 0.0;
    color: white;
  }

  &.hoverable {
    cursor: pointer;
    &:hover {
      background-color: white;
      .expand {
        opacity: 1.0;
      }
    }
  }

  .section {
    .display-flex;

    -ms-flex-align: center;
    -webkit-align-items: center;
    -webkit-box-align: center;
    align-items: center;

    @section-height: 17px;
    height: @section-height;
    margin-bottom: @section-height * 0.33333;

    &:last-child {
      margin-bottom: 0px !important;
    }

    &.selected-section {
      .selected-block{
        &:not(.activity-shown) {
          background-color: @action-color;
        }
        &.activity-shown{
          outline: 1px dotted @black-color;
        }
      }

    }    
  }

  .block {
    .background-transition;
    display: inline-block;
    background-color: @medium-color;
  }

  .vertical.block {
    height: 100%;
    width: 17px * 1.53333;
  }

  .horizontal.block {
    height: 76.47%;
    width: 17px * 0.7647 * 1.53333;
    margin-left: 3px;
  }
}

