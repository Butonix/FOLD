@context-count-size: 30px;



.context-count-details{
  display: none;
  height: @magic-styling-distance;
  background-color: @dark-color;
  position: absolute;
  left: 0;
  right: auto;
  bottom: @magic-styling-distance + 10px;
  min-width: 75px;
  &:after{
    @triangleSize: 14px;
    content:'';
    position: absolute;
    top: 100%;
    left: 55px;
    margin-left: -@triangleSize;
    width: 0;
    height: 0;
    border-top: solid @triangleSize @dark-color;
    border-left: solid @triangleSize transparent;
    border-right: solid @triangleSize transparent;
  }
}


.context-count-transparent-bridge{
  height: 10px;
  width: 100%;
  position: absolute;
  bottom: -10px;
}


.make-context-count-go-below{
  .context-count-details{
    top: @magic-styling-distance + 10;
    &:after{
      @triangleSize: 14px;
      top: auto;
      bottom: 100%;
      margin-left: -@triangleSize;
      border-top: none;
      border-bottom: solid @triangleSize @dark-color;
      border-left: solid @triangleSize transparent;
      border-right: solid @triangleSize transparent;
    }
  }
  .context-count-transparent-bridge{
    top: -10px;
  }
  .context-count-icon {
    &:first-child:nth-last-child(2) {
      &:before, &:after {
        display: none;
      }
    }
  }
}

.story{
  @media @not-mobile{
    &:nth-child(2), &:nth-child(3){
      .make-context-count-go-below;
    }
  }
  &:nth-child(1){
    .make-context-count-go-below;
  }
}

.context-count{
  overflow: visible;
  &:hover{
    .context-count-details{
      display: inline-block;
    }

  }
}

.context-count-text{
  display: none;
}

.context-count-icon {
  display: inline-block;
  position: relative;
  float: left;
  &:first-child:nth-last-child(2){
    float: right;
    &:before{
      content:'';
      position: absolute;
      left: -40px;
      bottom:-5px;
      height: @magic-styling-distance;
      width: 35px;
      background-color: @white-color;
    }
    &:after{
      content:'';
      position: absolute;
      left: -40px;
      bottom:-5px;
      height: @magic-styling-distance - 8px;
      border-top: 1px solid @medium-color;
      width: 35px;
    }
  }
  .size(@context-count-size);
  &:first-child{
    margin-left: 5px;
  }
  margin-right: 5px;
  margin-top: 5px;
  border-radius: @context-count-size;
  background-color: @action-color;
  svg{
    @bonus-size: 8px;
    .fg {
      fill: white;
    }
    .size(@context-count-size + @bonus-size);
    .center-over-parent-div;
  }

  &:hover{
    .fg{
      display: none;
    }
    .context-count-text {
      .FFMagdaCleanMonoWebProRegular;
      font-size: 13px;
      cursor: default;
      color: @white-color;
      .center-over-parent-div;
      display: inline;
    }
  }
}


