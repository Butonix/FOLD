}

article{
  background-color: inherit;
}

@nonMobileNarrativeSectionPadding: 30px;

.vertical-narrative-section {
  padding: @nonMobileNarrativeSectionPadding;

  @media @mobile {
    padding: 20px;
  }
  cursor: default;

  a {
    cursor: auto;
    padding: 4px 4px;
    pointer-events: none;
  }

  @border-width: 1px;
  border: @border-width solid transparent;

  position: relative;
  .width-transition;
  .height-transition;
  width: 100%;
  min-height: @horizontal-content-height + @remix-bar-height;
  color: @unselected-text-color;
  &.hidden-context-mode {
    min-height: @magic-styling-distance;
  }

  &.read, &.embed {
    margin-bottom: @card-spacing;
    @media @mobile {
      margin-bottom: @mobile-card-spacing;
    }
  }

  .context-button-container{
    .fg{
      fill: @action-color;
    }

    button{
      color: @action-color;
    }
    @media @mobile-or-thin-window {
      text-align: right;
      margin-top: 13px;
      svg{
        margin-right: 5px;
      }
    }
    @media @not-mobile-nor-thin-window {
      position: fixed;
      display: none;
      &.selected{
        display: block;
      }
      top: 150px;
      &.embed-mode{
        top: 30px;
      }
      right: 15px;
      svg{
        display: block;
        margin-bottom: 5px;
      }
      text-align: center;
      font-size: 14px;
    }
  }

  background-color: @unselected-background-color;

  .selected-styling {
    cursor: text;
    border: @border-width solid @inactive-color;
    color: @dark-color;
    opacity: 1.0;
    background-color: @selected-background-color;
    p {
      cursor: text;
    }
    a {
      // moved background-transition to specific cases so everything doesn't need to animate when a new card becomes selected
      cursor: pointer;

      pointer-events: auto;
      background-color: @unselected-link-color;
      color: inherit;
      &:hover {
        background-color: @medium-color;
        .background-transition;
      }
      &.active {
        background-color: @action-color;
        .background-transition;
        color: white;
        cursor: inherit;
        pointer-events: none;

      }
      &.placeholder {
        background-color: @social-color;
        .background-transition;
        color: white;
      }
    }
  }

  &.selected {
    .selected-styling;
  }

  &.hidden-context-mode{
    .selected-styling;
    &.selected{
      @media @not-mobile-nor-thin-window{
        border-left-color: @action-color;
        &:before {
          content:"";
          display:block;
          position: absolute;
          top: 0;
          bottom: 0;
          left:0;
          width:0;
          border-left: 3px solid @action-color;
        }
      }
    }
  }

  .edge-to-edge-mode{
    .selected-styling;
    border: none !important;
    .random-story{
      background-color: @social-color;
    }
  }

  @media @mobile-or-thin-window {
    .edge-to-edge-mode;
  }

}

.narrative-section {
  float: left;
  display: inline-block;
  &.new-section{
    padding: 0 15px;
  }

  div.vertical-nav-container {
    position: absolute;
    top: 24px;
    right: 15px;

    img.arrow {
      margin-right: 5px;
      display: inline;
      border-radius: 17.5px;
      cursor: pointer;
      &:hover {
        background-color: @action-color;
      }
    }
  }

  @normalContentHeight: 232px;
  @titleHeight: 23px;
  @titleMarginBottom: 15px;

  div.title {
    line-height: @titleHeight;
    font-size: 20px;
    .FFMarkWebBold;
    margin-bottom: @titleMarginBottom;
  }

  div.content {
    overflow: hidden;
    word-wrap: break-word;
    .content-font;
    min-height: @normalContentHeight + @titleHeight + @titleMarginBottom;
    &.minimized {
      height: 0;
      display: none;
    }
  }
  &.has-title {
    .content {
      min-height: @normalContentHeight;
    }
  }

  &.hidden-context-mode {
    .content {
      min-height: @magic-styling-distance !important;
    }
  }
}


.display-context-section{
  height: 100%;
}

div.image-section {
  .size-to-fit;
  position: relative;
  line-height: @horizontal-content-height;
  @media @resizing-context {
    line-height: 0;
  }
  background-color: @black-color;
  text-align: center;
  &:hover, &.always-show-caption{
    div.text-content{
      background-color: rgba(0, 0, 0, 0.8);
      opacity: 0.8;
    }
  }


  .expand-icon-box{
    opacity: 0;
    background-color: rgba(0, 0, 0, 0.8);
    position: absolute;
    left: 8px;
    bottom: 8px;
    .size(40px);
    .fg{
      fill: @white-color;
    }
    svg{
      .size(20px);
      .center-over-parent-div;
    }
  }
  &.show-expand-icon{
    cursor: pointer;
    &:hover{
      .expand-icon-box{
        opacity: 1;
      }
    }
  }



  img.giphy-logo {
    position: absolute;
    bottom: 10px;
    right: 0px;
    width: 150px;
  }
  
  .text-content, .expand-icon-box {
    .opacity-background-transition;
  }

  textarea.text-content {
    position: absolute;
    padding: 8px;
    top: 0;
    left: 0;
    background-color: rgba(0, 0, 0, 0.8);
    opacity: 0.8;
    color: white;
  }

  div.attribution {
    margin-top: -30px;
    line-height: 30px;
    padding-right: 8px;
    text-shadow: 1px 1px @dark-color;
    text-align: right;
    color: white;
    font-size: 13px;

    i.fa-flickr {
      margin-right: 2px;
    }

    a {
      &:hover {
        color: @action-color;
      }
    }

    span.separator {
      margin-left: 3px;
      margin-right: 6px;
    }
  }

  div.text-content {
    @font-size: 14px;
    font-size: @font-size;
    line-height: 20px;
    .text-align-start;
    position: absolute;
    top: 0;
    width: 100%;
    padding: 8px;
    opacity: 0;
    background-color: rgba(0, 0, 0, 0);
    color: white;
    min-height: 36px;

  }

  .looping-video{
    .size-to-fit;
  }

  img, video {
    width: auto;
    max-width: 100%;
    margin: auto;
    vertical-align: middle;
    display: inline-block;
    max-height: 100%;
  }
}

div.broken-section {
  height: @horizontal-content-height;
  @media @resizing-context {
    height: 100%;
  }
  width: 100%;
  background-color: @inactive-color;
  position: relative;
}

div.vertical-narrative {
  position: absolute;

  top: 475px;
  margin-top: -65px;
  z-index: 10;
  float: left;
  width: 400px;
  &.fixed {
    position: fixed;
    top: 334px;
  }

  &.free-scroll{
    top: 471px;
  }

  &.sandwich-mode{
    top: 185px;
  }

  left: @vertical-left;

  &.hidden-context-mode{
    @media @not-mobile-nor-thin-window{
      width: calc(100% ~'-' (@card-spacing * 2 + @magic-styling-distance)) !important;
      left: @card-spacing;
    }
    @media @mobile-or-thin-window {
      width: 100% !important;
      left: 0;
    }
    background-color: inherit;

    &.embed-mode{
      position: static;
      margin-top: @card-spacing;
      @media @not-mobile-nor-thin-window {
        margin-left: @card-spacing;
      }
    }
  }

  @media @mobile{
    position: static;
    margin-top: @magic-styling-distance + 15px;
    float: none;
    width: auto !important;
  }
}

div.oec-section {
  background-color: white;
}

div.link-section {
  position: relative;
  height: @horizontal-content-height;
  @media @resizing-context {
    height: 100%;
  }
  border-bottom: 7px solid @action-color;

  overflow-y: hidden;

  div.top {
    .thumbnail {
      position: relative;
      &.left {
        float: left;
        width: 130px;
        height: 130px;
        overflow: hidden;
        background-color: @black-color;
        img{
          min-height: 100%;
