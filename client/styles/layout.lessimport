@highdensity: ~"only screen and (-webkit-min-device-pixel-ratio: 1.5)",
~"only screen and (min--moz-device-pixel-ratio: 1.5)",
~"only screen and (-o-min-device-pixel-ratio: 3/2)",
~"only screen and (min-device-pixel-ratio: 1.5)";
@smaller-mobile:      ~"only screen and (max-device-width: 330px)";
@mobile:      ~"only screen and (max-device-width: 529px)";
@not-mobile:  ~"(min-device-width: 530px)";
@thin-window:  ~"(max-width: 400px)";
@mobile-or-thin-window: ~"only screen and (max-device-width: 529px)", ~"(max-width: 400px)";
@not-mobile-nor-thin-window:  ~"(min-device-width: 530px) and (min-width: 400px)";
@resizing-context:      ~"only screen and (max-device-width: 529px)", ~"(max-height: 570px)", ~"(max-width: 685px)";
@tablet:      ~"only screen and (min-device-width: 530px) and (max-device-width: 949px)";
@desktop:     ~"only screen and (min-device-width: 950px) and (max-device-width: 1128px)";
@desktop-xl:  ~"only screen and (min-device-width: 1129px)";
@desktop-xtall:  ~"only screen and (min-height: 1129px)";

.animation(@string){
  -webkit-animation: @string;
  -moz-animation: @string;
  animation: @string;
}

.transform(@string){
  -webkit-transform: @string;
  -moz-transform: @string;
  -ms-transform: @string;
  -o-transform: @string;
  transform: @string;
}

.filter(@string){
  -webkit-filter: @string;
  -moz-filter: @string;
  -o-filter: @string;
  -ms-filter: @string;
  filter: @string;
}


.flex(@string){
  -webkit-flex: @string;
  -ms-flex: @string;
  flex: @string;
}


.display-flex{
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
}


.linear-gradient (@direction, @startColor, @endColor) {
  background: -webkit-linear-gradient(@direction, @startColor, @endColor);
  background: -moz-linear-gradient(@direction, @startColor, @endColor);
  background: -ms-linear-gradient(@direction, @startColor, @endColor);
  background: -o-linear-gradient(@direction, @startColor, @endColor);
  background: linear-gradient(@direction, @startColor, @endColor);
}

.transition (@value1,@value2:X,...)
{
  @value: ~`"@{arguments}".replace(/[\[\]]|\,\sX/g, '')`;
  -webkit-transition: @value;
  -moz-transition: @value;
  -o-transition: @value;
  transition: @value;
}
.box-shadow(@value1,@value2:X,...)
{
  @value: ~`"@{arguments}".replace(/[\[\]]|\,\sX/g, '')`;
  -webkit-box-shadow: @value;
  -moz-box-shadow: @value;
  -o-box-shadow: @value;
  box-shadow: @value;
}

.left-transition{
  .transition(left 0.2s ease-in);
}

.width-transition{
  .transition(width 0.2s ease-in);
}

.height-transition{
  .transition(height 0.2s ease-in);
}

.text-shadow-transition{
  .transition(text-shadow 0.2s ease-in);
}

.background-transition{
  .transition(background-color 0.2s ease-in);
}

.border-transition {
  .transition(border 0.2s ease-in);
}

.color-transition {
  .transition(color 0.2s ease-in);
}

.opacity-transition {
  .transition(opacity 0.3s ease-in);
}

.opacity-background-transition {
  .transition(opacity 0.3s ease-in, background-color 0.3s ease-in);
}

.shadow-transition {
  .transition(box-shadow 0.35s ease-in);
}

.fill-transition {
  .transition(fill 0.2s ease-in);
}

.stroke-transition {
  .transition(stroke 0.2s ease-in);
}

.top-transition {
  .transition(top 0.2s ease-in);
}


.no-select {
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  cursor: default;
}

.size-to-fit {
  width: 100%;
  height: 100%;
}

.hide {
  visibility: hidden !important; // could also use left: -9999px. better than display:none because it still has a non-zero size
  opacity: 0 !important;
  pointer-events: none !important;
}


.display-none {
  display: none;
}


.padding-sides(@s){
