svg.icons {
  fill: @dark-color;
}
svg.remix-icon{
  fill: @remix-color;
}
svg.remix-icon-square{
  fill: @remix-color;
}
.favorite-button{
  background-color: transparent;
  border: none;
  &.favorite{
    svg{
      fill: @color-light-grey;
      &:hover{
        fill: @favorite-color;
      }
    }
  }
  &.unfavorite{
    svg{
      fill: @favorite-color;
    }
  }
}
